    <%= javascript_include_tag ['jquery', 'jquery-ui.min', 'autoresize.jquery.min', 'rails', 'form', 'admin'] %>
    <%= csrf_meta_tag %>
    
    <%= yield(:head) %>
    
  </head>
  <body>
    <div id="container"><div id="wrapper">
      <%= render 'admin/shared/toplinks' %>
      <%= render 'admin/shared/nav' %>
      <%= render_subnav %>
      
      <div id="main"><div id="m">
        <%= render 'shared/flashbox' %>          
        <%= yield %>
      </div></div>
      
    </div></div>
  </body>
</html>